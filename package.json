{
  "name": "lore",
  "displayName": "Lore â€” Enriched inline comments",
  "publisher": "annot8-lore",
  "version": "0.0.1",
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lore.createEnrichedComment",
        "title": "Lore: Weave new Lore"
      },
      {
        "command": "lore.enableHighlights",
        "title": "Lore: Summon Highlights"
      },
      {
        "command": "lore.disableHighlights",
        "title": "Lore: Seal Highlights"
      },
      {
        "command": "lore.editComment",
        "title": "Lore: Patch the Lore"
      },
      {
        "command": "lore.previewMarkdown",
        "title": "Lore: View Lore Entry"
      },
      {
        "command": "lore.listAllEntries",
        "title": "Lore: Reveal all Entries"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "lore.editComment",
          "when": "false"
        },
        {
          "command": "lore.previewMarkdown",
          "when": "false"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -p ./ -w",
    "pretest": "pnpm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/marked": "^6.0.0",
    "@types/node": "^18.0.0",
    "@types/uuid": "^11.0.0",
    "@types/vscode": "^1.79.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "marked": "^17.0.1",
    "uuid": "^13.0.0"
  }
}
